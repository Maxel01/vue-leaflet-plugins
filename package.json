{
  "name": "@maxel01/vue-leaflet-plugins",
  "version": "0.0.1-alpha.0",
  "license": "MIT",
  "public": true,
  "type": "module",
  "sideEffects": false,
  "main": "dist/vue-leaflet-plugins.cjs.js",
  "unpkg": "dist/vue-leaflet-plugins.umd.js",
  "module": "dist/vue-leaflet-plugins.es.js",
  "types": "./dist/src/plugins.d.ts",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:maxel01/vue-leaflet-plugins.git"
  },
  "scripts": {
    "dev": "pnpm --filter playground dev",
    "dev:vue": "pnpm --filter playground-vue dev",
    "test": "vitest --coverage",
    "build": "vue-tsc --noEmit && vite build",
    "build-only": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier --write src/ tests/",
    "docs:generate": "node docs/scripts/generate-docs.js",
    "docs:dev": "node docs/scripts/generate-docs.js && vitepress dev docs",
    "docs:build": "node docs/scripts/generate-docs.js && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "release": "release-it"
  },
  "peerDependencies": {
    "@maxel01/vue-leaflet": "^1.0.0-beta",
    "leaflet": "^2.0.0-alpha",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@maxel01/vue-leaflet": "^1.0.0-beta",
    "@release-it/conventional-changelog": "^8.0.2",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24.0.14",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "^10.3.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "release-it": "^17.11.0",
    "ts-morph": "^26.0.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.17",
    "vue-docgen-api": "^4.79.2",
    "vue-tsc": "^2.2.12"
  }
