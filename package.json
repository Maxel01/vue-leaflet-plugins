{
  "name": "@maxel01/vue-leaflet-plugins",
  "version": "0.0.4",
  "license": "MIT",
  "public": true,
  "type": "module",
  "sideEffects": false,
  "main": "dist/vue-leaflet-plugins.cjs.js",
  "unpkg": "dist/vue-leaflet-plugins.umd.js",
  "module": "dist/vue-leaflet-plugins.es.js",
  "types": "./dist/src/plugins.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "LICENCES/"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:maxel01/vue-leaflet-plugins.git"
  },
  "scripts": {
    "dev": "pnpm --filter playground dev",
    "dev:vue": "pnpm --filter playground-vue dev",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "build": "vue-tsc --noEmit && vite build",
    "build-only": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint ./src --fix",
    "lint:check": "eslint ./src --max-warnings=0",
    "format": "prettier --write src/ tests/",
    "format:check": "prettier --check src/ tests/",
    "docs:generate": "node docs/scripts/generate-docs.js",
    "docs:dev": "node docs/scripts/generate-docs.js && vitepress dev docs",
    "docs:build": "node docs/scripts/generate-docs.js && vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "release": "release-it"
  },
  "peerDependencies": {
    "@maxel01/vue-leaflet": "^1.0.0-beta.2",
    "leaflet": "2.0.0-alpha",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@maxel01/vue-leaflet": "^1.0.0-beta.6",
    "@release-it/conventional-changelog": "^10.0.4",
    "@types/leaflet": "^1.9.20",
    "@types/leaflet.markercluster": "^1.5.6",
    "@types/node": "^25.0.3",
    "@types/simpleheat": "^0.4.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "^1.6.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.3",
    "jsdom": "^27.4.0",
    "prettier": "^3.7.4",
    "release-it": "^19.2.2",
    "ts-morph": "^27.0.2",
    "typescript": "~5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "2.0.0-alpha.15",
    "vitest": "^3.2.4",
    "vue": "^3.5.26",
    "vue-docgen-api": "^4.79.2",
    "vue-tsc": "^3.2.1"
  },
  "dependencies": {
    "leaflet.markercluster": "^1.5.3",
    "simpleheat": "^0.4.0"
  }
}
